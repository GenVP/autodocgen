	"ШаблонЗаголовокДляКонстант": "<h4><span style=\"color: rgb(51,102,255);\">%1</span></h4>",
	"ШаблонШапкаТЧ": "<table><tbody><tr><th>&nbsp;</th><th>Параметр</th><th>Обязательный</th><th>Тип</th><th>Описание</th></tr>",
	"ШаблонШапкаТЧДляКонстант": "<table><tbody><tr><th>Константа</th><th>Тип</th><th>Описание</th></tr>",
	"ШаблонСтрокаТЧ": "<tr><td>%1</td><td colspan=\"1\">%2</td><td colspan=\"1\">%3</td><td colspan=\"1\">%4</td><td colspan=\"1\">%5</td></tr>",
	"ШаблонСтрокаТЧДляКонстант": "<tr><td colspan=\"1\">%1</td><td colspan=\"1\">%2</td><td colspan=\"1\">%3</td></tr>",
	"ШаблонПодвалТЧ": "</tbody></table>",
